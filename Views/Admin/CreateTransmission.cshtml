@{
     Layout = "~/Views/Shared/_Layout.cshtml";
}

@using CarBookingApp.ViewModels
@model TransmissionViewModel

<form method="post" action="@Url.Action("CreateTransmission", "Admin")">
     @Html.AntiForgeryToken()

     <div class="form-group">
          <label for="TransmissionName">Transmission Name</label>
          <input type="text" class="form-control" id="TransmissionName" name="TransmissionName">
     </div>

     <button type="submit" class="btn btn-primary">Create</button>
</form>